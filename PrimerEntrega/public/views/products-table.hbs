<h3 class="title">Lista de Productos</h3>
{{#if products.length}}
<table class="product-list">
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Stock</th>
        <th>Foto</th>
    </tr>
    {{#each products}}
    <tr>
        <td>{{this.nombre}}</td>
        <td>${{this.precio}}</td>
        <td>{{this.stock}}</td>
        <td><img width="48" src={{this.foto}} alt="image"></td>
    </tr>
    {{/each}}
</table>
{{else}}
<div>
    No hay productos
</div>
{{/if}}